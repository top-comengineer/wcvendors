{
  "name": "wc-vendors",
  "version": "2.4.2",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@wordpress/stylelint-config": "^20.0.2",
    "babel-eslint": "^10.0.1",
    "cypress": "^3.8.3",
    "del": "latest",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "latest",
    "gulp-cache": "latest",
    "gulp-clean-css": "latest",
    "gulp-concat": "latest",
    "gulp-ignore": "latest",
    "gulp-jshint": "latest",
    "gulp-livereload": "latest",
    "gulp-load-plugins": "latest",
    "gulp-notify": "latest",
    "gulp-rename": "latest",
    "gulp-ruby-sass": "latest",
    "gulp-sass": "^4.1.1",
    "gulp-sort": "latest",
    "gulp-uglify": "latest",
    "gulp-wp-pot": "latest",
    "husky": "^1.3.1",
    "jshint": "latest",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.19.1",
    "pump": "latest",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "tiny-lr": "latest",
    "stylelint-config-prettier-scss": "^0.0.1"
  },
  "scripts": {
    "start": "npm install && gulp",
    "build": "gulp build",
    "dev": "gulp && gulp watch"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "singleQuote": true,
    "bracketSpacing": true,
    "semi": true,
    "useTabs": true,
    "parser": "babel",
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "parser": "scss"
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.scss": [
      "prettier --write",
      "stylelint",
      "git add"
    ],
    "**/*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "stylelint-config-prettier-scss": "^0.0.1"
  }
}
